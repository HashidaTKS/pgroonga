require_relative "../pgroonga-package-task"

class PostgreSQL12PGroongaPackageTask < PGroongaAptPackageTask
  def initialize
    super("postgresql-12-pgroonga")
  end

  def apt_targets_default
    [
      "debian-stretch",
      "debian-buster",
    ]
  end
end

task = PostgreSQL12PGroongaPackageTask.new
task.define
