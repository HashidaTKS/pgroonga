require_relative "../pgroonga-package-task"

class PostgreSQL10PGroongaPackageTask < PGroongaPackageTask
  def initialize
    super("10")
  end

  def apt_targets_default
    [
      "debian-stretch",
      "debian-stretch-i386",
    ]
  end

  def yum_targets_default
    [
      "centos-6",
      "centos-7",
    ]
  end
end

task = PostgreSQL10PGroongaPackageTask.new
task.define
